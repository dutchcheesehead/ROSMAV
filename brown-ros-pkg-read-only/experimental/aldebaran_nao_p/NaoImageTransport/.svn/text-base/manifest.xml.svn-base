<package>
  <description brief="NaoImageTransport">
     NaoImageTransport takes the native NAO video stream (/eyes) and republishes
it as a ROS compatible image transport.
  </description>
  <author>Graylin Trevor Jay</author>
  <email>tjay@cs.brown.edu</email>
  <license>BSD</license>

  <review status="unreviewed" notes=""/>

  <depend package="opencv2"/>
  <depend package="cv_bridge"/>
  <depend package="image_transport"/>

  <export>
    <cpp cflags="-I${prefix}/msg/cpp"/>
  </export>


</package>


